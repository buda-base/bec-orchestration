{
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/bec/worker-*.log",
            "log_group_name": "/bec/workers",
            "log_stream_name": "{instance_id}/{filename}",
            "timezone": "UTC",
            "timestamp_format": "%Y-%m-%dT%H:%M:%S.%f"
          }
        ]
      }
    },
    "force_flush_interval": 15
  }
}